#include<stdio.h>
int ans,sol;
int main(){
    printf("輸入答案:")；
    scanf("%d",&ans)
    while(sol!=ans){
    if(sol>ans){
    printf("比1大，比%d小",sol)；

    }
    if(sol<ans){
    printf("比%d大，比100小",sol)；
   }

}
    return 0;
}

int y=1;
int x=1;
int main(){
    while (y<=9){
      while (x<=9){
         printf("%d*%d=%d\t",y,x,y*x );
         x++;
         }
      y++;
      x=1;
      printf("\n");
      }
return 0;
}

