//九九乘法表
#include<stdio.h> 
#include<stdlib.h>
#include<time.h>

int main(){
	int i,j;
	int A[10][10];
	
	for(i=1;i<10;i++){
		for(j=1;j<10;j++){
			A[i][j]=i*j;
		}
	}	
	for(i=1;i<10;i++){
		for(j=1;j<10;j++){
			printf("%d*%d=%2d ",i,j,A[i][j]);
		}
		printf("\n");
	}
	return 0;
}


#include<stdio.h> 
#include<stdlib.h>
#include<time.h>

int main(){
	int i,j;
	int A[10][10];
	
	for(i=0;i<9;i++){
		for(j=0;j<9;j++){
			A[i][j]=(i+1)*(j+1);
		}
	}	
	for(i=0;i<9;i++){
		for(j=0;j<9;j++){
			printf("%d*%d=%2d ",i+1,j+1,A[i][j]);
		}
		printf("\n");
	}
	return 0;
}
