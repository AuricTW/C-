#include<stdio.h> 

int main(void){
	long long int A;
	long long int B;
	long long int X;
	X = A*B;
	printf("輸入A值");
	scanf("%lld",&A);
	printf("輸入B值");
	scanf("%lld",&B);
    printf("%lld * %lld \n",A,B);
    printf("%lld",X);
    
	return 0;
}

char a[2]={'A','B'};

or

char a[]="AB";



#include<stdio.h> 

int main(void){
	long long int A;
	long long int B;
	long long int X;
	
	printf("輸入A值");
	scanf("%lld",&A);
	printf("輸入B值");
	scanf("%lld",&B);
	X = A*B;
    printf("%lld*%lld \n",A,B);
    printf("%lld",X);
    
	return 0;
}


#include <stdio.h>
int main(){
	int a,b;
	printf("請輸入變數1:");
	scanf("%d",&a);
	printf("請輸入變數2:");
	scanf("%d",&b);
    
	if(a>3 && b>5){
	    printf("%d",a+b);
	}
	else if(a>3 && b<=5){
		printf("%d",a-b);
	}
	
	else if(a<3 && b>7){
		printf("%d",a*b);
	}
	else if(a<3 && b<=7){
		printf("%d",a/b);
	}
	else if(a==3){
		printf("No");
	}
	return 0;
}
