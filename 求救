

#include<stdio.h>
#include <string.h>
#include<stdlib.h>
int main(){
	char A[150],B[150],X[1];
	int N,A1,B1; 
	scanf("%s",&A);
	do {
		scanf("%d%s",&X,&B);  
	
		if(X[0]=='0'){
			B1=sizeof(B)-1;
			for(int i=0;i<=sizeof(A)-1;i++){
			A[i+B1]=A[i];
			}
			for(int j=0;j<=B1;j++){    //複製陣列 
		    	A[j]=B[j];
			}
			A1=sizeof(A)-1;  //新陣列長度 
			int B[150]={0};	
		}
		
	    else if(X[0]=='1'){
			B1=sizeof(B)-1;	
			
			for(int j=0;j<=B1;j++){    //複製陣列 
		    	A[j]=B[j];
			}
			
			A1=sizeof(A)-1;  //新陣列長度 
			int B[150]={0};
		}
		
  
	}while(1);
	printf("%s",A);
	return 0;
} 
