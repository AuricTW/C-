#include<stdio.h>
#include<stdlib.h>
int main(){
	int n,i,j;
	char name[100];
	
	printf("輸入名字:");
	scanf("%s",&name);
	
	printf("輸出顯示次數:");
	scanf("%d",&n);
	
	do{
		i++;
		printf("你的名字第%d次:%s\n",i,name); 
		
	}while(i!=n);
	 
	return 0;
}
