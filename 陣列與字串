#include<stdio.h>
#include<stdlib.h>
#include<time.h>
int main(){
	srand(time(0));
	int counter[6]={0};
	int i,j;
	for(i=1;i<=6000;i++){
		int dice=rand() % 6 + 1;
		for(j=1;j<=6;j++){
			if(dice==j){	
				counter[j-1]++;
			}
		}
	}
	for(j=0;j<6;j++){
		printf("%d: %d\n",j+1,counter[j]);
	}
	return 0;
}

#include<stdio.h>
#include<stdlib.h>
#include<time.h>

int main(){
	printf("商品1:75元\n商品2:100元\n商品3:200元\n商品4:500元\n商品5:1500元\n");
	int total=0,id;
	int price[5]={75, 100, 200, 500, 1500};
	do{
	printf("輸入商品編號");
	scanf("%d",&id);
	total+=price[id-1];
	}while(id!=0);
	printf("total:%d元",total);
	return 0;
} 

#include<stdio.h>
#include<stdlib.h> 
#include<time.h>

int main(){
	int A[11]={0};
	int dice,i;
	srand(time(0));
	
	for(i=1;i<=100000;i++){
		int dice1=rand()%6+1;
		int dice2=rand()%6+1;
		dice=dice1+dice2;
		A[dice-2]++;
	}
	for(i=2;i<=12;i++){
		printf("%d點:%d次\n",i,A[i-2]);
	}
	return 0;
}

失敗
#include<stdio.h>
#include<stdlib.h> 
#include<time.h>
int str_len(char str[]){
	int i=0;
	while(str[i] != '\0'){
		i++;
	}
	return i;
}
int main(){
	char str[10] = "Hello World";
	printf("Length: %zu\n",sizeof(str));
	printf("Length: %d\n",str_len(str));
	return 0;
}
